    "url-loader": "^0.6.2",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^3.8.1"
  },
  "babel": {
    "plugins": [
      "transform-runtime",
      "typecheck",
      "transform-class-properties",
      "transform-decorators-legacy",
      "add-module-exports",
      "import-asserts",
      "syntax-async-functions",
      "transform-regenerator",
      "transform-export-extensions"
    ],
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "require": [
      "babel-core/register",
      "babel-polyfill",
      "mocha"
    ],
    "include": [
      "src/**"
    ],
    "exclude": [
      "src/**/__tests__/*.js"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "html",
      "text",
      "text-lcov"
    ]
  }
